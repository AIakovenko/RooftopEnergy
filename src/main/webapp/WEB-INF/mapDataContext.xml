       <bean id="Overijssel" class="nl.rooftopenergy.bionic.rest.util.MapEntity">
              <constructor-arg name="name" value="Overijssel"/>
              <constructor-arg name="y" value="-1150"/>
              <constructor-arg name="x" value="1200"/>
       </bean>

       <bean id="dutchProvinceList" class="nl.rooftopenergy.bionic.rest.util.MapData">
              <constructor-arg>
              <list>
                     <ref bean="Friesland"/>
                     <ref bean="Noord-Holland"/>
                     <ref bean="Groningen"/>
                     <ref bean="Flevoland"/>
                     <ref bean="Zeeland"/>
                     <ref bean="Noord-Brabant"/>
                     <ref bean="Utrecht"/>
                     <ref bean="Zuid-Holland"/>
                     <ref bean="Drenthe"/>
                     <ref bean="Gelderland"/>
                     <ref bean="Limburg"/>
                     <ref bean="Overijssel"/>
              </list>
